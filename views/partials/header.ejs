<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="tools">
    <% if (locals.loggedin) { %>
    <a href="/account/" title="Manage your account">Welcome <%= locals.accountData.account_firstname %> |</a>

    <% if (locals.accountData.account_type === "Admin") { %>
    <a href="/admin/" title="Admin Dashboard">Dashboard |</a>
    <% } %>

    <a href="/account/logout" title="Click to log out">Logout</a>

    <% } else { %>
    <a href="/account/login" title="Click to log in">Login</a>
    <% } %>
  </div>
</header>